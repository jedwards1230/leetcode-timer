{
    "name": "leetcode-timer",
    "version": "1.0.3",
    "description": "Web extension for timing LeetCode problems",
    "webExt": {
        "verbose": true
    },
    "scripts": {
        "start": "web-ext run -p default",
        "watch": "webpack -w --mode development",
        "build": "webpack -c webpack.config.js --mode development",
        "build-release": "webpack -c webpack.config.js --mode production"
    },
    "repository": "https://github.com/jedwards1230/leetcode-timer",
    "author": "Justin Edwards",
    "license": "MIT",
    "private": true,
    "dependencies": {
        "dom-chef": "^5.1.0",
        "webextension-polyfill": "^0.9.0"
    },
    "devDependencies": {
        "@sindresorhus/tsconfig": "^3.0.1",
        "@types/firefox-webext-browser": "^94.0.1",
        "copy-webpack-plugin": "^11.0.0",
        "ts-loader": "^9.3.1",
        "typescript": "^4.7.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    }
}